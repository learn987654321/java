import java.util.Scanner;
class Factorial
{
	public static void main(String args[])
        {
            long pro=1;
            System.out.println("enter the number");
            Scanner ob=new Scanner(System.in);
            int j=ob.nextInt();

            for(int i=1; i<=j; i++)
            {
                pro*=i;
            }
            System.out.println("factorial of number:"+pro);
        }
}
